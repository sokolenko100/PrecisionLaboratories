{
  "name": "PrecisionLaboratories",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean-cache": "cd android && ./gradlew cleanBuildCache && cd .. && watchman watch-del-all && rm -rf $TMPDIR/react-native-packager-cache-* &&rm -rf $TMPDIR/metro-bundler-cache-* && rm -rf node_modules/ && npm cache clean --force && npm install && npm start -- --reset-cache",
    "clean-metro-cache": "react-native start -- --reset-cache",
    "full-reinstall": "npm run clean-cache && npm i && cd ios && pod install && cd ..",
    "full-install": "npm install && cd ios && pod install && cd ..",
    "clean-build": "\\rm -fr ./node_modules && \\rm -fr dist/* && \\rm -fr ios/build ios/Pods ios/KScoreApp.xcarchive && \\rm -fr android/build android/app/build",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.19.0",
